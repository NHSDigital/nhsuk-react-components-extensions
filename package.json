    "react-dom": "^16.9.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-input-mask": "^2.0.4"
  },
  "scripts": {
    "build": "yarn build:dist && yarn build:lib && yarn build:styles",
    "build:dist": "rollup -c",
    "build:lib": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline && tsc --emitDeclarationOnly",
    "build:styles": "node-sass --recursive --importer=node_modules/node-sass-tilde-importer --output css src",
    "cleanup": "bash scripts/cleanup.sh",
    "lint": "stylelint src/**/*.scss && eslint --fix -c .eslintrc.js --ext .ts --ext .tsx src/*.ts src/components/**/*.ts src/components/**/*.tsx",
    "prebuild": "yarn lint && yarn test --coverage && yarn cleanup",
    "storybook": "start-storybook",
    "test": "jest",
    "test:ci": "jest --coverage"
  },
  "files": [
    "dist",
    "lib",
    "css"
  ],
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "main": "dist/index.js"
}
